msgid ""
"    -uS, -uB, -uC, -uM:  accept/report in units of sectors/blocks/cylinders/"
"MB"
msgstr ""
"    -uS, -uB, -uC, -uM:  Accepta/reporta en unitats de sectors/blocs/"
"cilindres/MB"

